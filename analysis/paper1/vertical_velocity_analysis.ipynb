  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "for H in [3,5,10,15,20]:\n",
    "    for prefix in ['w_h2o_', 'w_tc_', 'u*']:\n",
    "        df[f\"{prefix}_{H}m_c_fit_dr_minus_pf\"] = (df[f\"{prefix}_{H}m_c_fit_dr\"] - df[f\"{prefix}_{H}m_c_fit_pf\"])\n",
    "        df[f\"{prefix}_{H}m_c_fit_dr_minus_pf10\"] = (df[f\"{prefix}_{H}m_c_fit_dr\"] - df[f\"{prefix}_{H}m_c_fit_pf10\"])\n",
    "        "
   ]
