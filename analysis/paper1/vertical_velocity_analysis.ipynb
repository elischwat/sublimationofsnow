    "def get_boxplot(\n",
    "        src, \n",
    "        variable,\n",
    "        limits,\n",
    "        y_domain,\n",
    "        text_y_loc,\n",
    "        bins,\n",
    "        bin_labels,\n",
    "    ):\n",
    "    src = src[\n",
